{
  "name": "react-silk-ui",
  "version": "0.1.4",
  "license": "MIT",
  "main": "dist/react-silk-ui.js",
  "module": "dist/react-silk-ui.es.js",
  "author": "Young Tailors <team@procolla.com>",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist && tsc",
    "build": "rollup -c",
    "dev": "concurrently \"rollup -cw\" \"tsc -w\"",
    "tsc": "tsc",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "jest --config \"./config/jest.config.js\"",
    "test:watch": "jest --watch --config \"./config/jest.config.js\"",
    "test:coverage": "jest --coverage --config \"./config/jest.config.js\"",
    "test:coverage-ci": "jest --ci --runInBand --coverage --config \"./config/jest.config.js\""
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-art": "^16.8.4",
    "react-native-web": "^0.10.1"
  }
}
